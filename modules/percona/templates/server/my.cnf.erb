[mysqld]
datadir=/var/lib/mysql
user=mysql
log_error=error.log
binlog_format=ROW
innodb_locks_unsafe_for_binlog=1
innodb_autoinc_lock_mode=2
innodb_log_file_size=64M
log_slave_update=1
log-bin
relay_log_purge=0
server_id=<%=server_id%>
replicate-ignore-db=mysql

[mysql]
prompt="<%=hostname %> mysql> "
